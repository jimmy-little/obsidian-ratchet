{
  "name": "obsidian-ratchet",
  "version": "0.1.0",
  "description": "Obsidian plugin",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "install:vault": "node install.mjs",
    "build:install": "npm run build && npm run install:vault",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "release": "node release.mjs"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin"
  ],
  "author": "Jimmy Little",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
